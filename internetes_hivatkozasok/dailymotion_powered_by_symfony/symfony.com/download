                    <script type="text/javascript">
                        var updateSymfonyUrl = function ()
                        {
                            var url = $("#download-form").attr('action') + '?v=' + $("#download-version").val();
                            $("#download-url a").attr('href', url);
                            $("#download-url a").text(url);
                        };

                        $(document).ready(function () {
                            updateSymfonyUrl();
                            $("#download-version").change(updateSymfonyUrl);
                            $("#download-url a").click(function () {
                                _gaq.push(['_trackEvent', 'Downloads', 'Symfony SE', $("#download-version").val()]);
                            });
                            $("#download-form").submit(function () {
                                _gaq.push(['_trackEvent', 'Downloads', 'Symfony SE', $("#download-version").val()]);
                            });
                        });
                    </script>
