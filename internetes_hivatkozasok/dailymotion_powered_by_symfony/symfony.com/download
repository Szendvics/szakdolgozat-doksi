                                        <option value="Symfony_Standard_2.2.1.tgz">Symfony Standard 2.2.1 without vendors (.tgz)</option>
                                        <option value="Symfony_Standard_2.2.1.zip">Symfony Standard 2.2.1 without vendors (.zip)</option>

                                        <option value="Symfony_Standard_Vendors_2.1.9.tgz">Symfony Standard 2.1.9 (.tgz)</option>
                                        <option value="Symfony_Standard_Vendors_2.1.9.zip">Symfony Standard 2.1.9 (.zip)</option>
                                        <option value="Symfony_Standard_2.1.9.tgz">Symfony Standard 2.1.9 without vendors (.tgz)</option>
