atom_id = acl.id.text
id = atom_id.split('/')[-1]
</pre>
        </li>
        <li>
<pre class="prettyprint">
# Retrieve the ACL entry role.
role = acl.role.value
</pre>
        </li>
        <li>
<pre class="prettyprint">
# Retrieve the ACL entry scope type.
scope_type = acl.scope.type
</pre>
        </li>
        <li>
<pre class="prettyprint">
