String gadgetHeight = gadget.<a href="/gdata/javadoc/com/google/gdata/data/calendar/WebContent#getHeight()">getHeight</a>();

for (Map.Entry&lt;String, String&gt; gadgetPreference :
         gadget.<a href="/gdata/javadoc/com/google/gdata/data/calendar/WebContent#getGadgetPrefs()">getGadgetPrefs</a>()) {
  String preferenceKey = gadgetPreference.getKey();
  String preferenceValue = gadgetPreference.getValue();
}
